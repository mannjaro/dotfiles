Host indigo
    Hostname www.mochicat.dev 
    User ubuntu
    Port 22
    IdentityFile ~/.ssh/id_ed25519

Host github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/github

Host kit-wp
    HostName www.monodukuri.kit.ac.jp
    User mannjaro
    Port 22
    IdentityFile ~/.ssh/id_ed25519

Host puffer
    HostName se.is.kit.ac.jp
    ControlMaster auto
    ControlPersist 10m

Host ayu
    HostName ayu.se.is.kit.ac.jp
    ProxyJump puffer

