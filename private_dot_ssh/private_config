Host sel # ← just an identifier
    HostName se.is.kit.ac.jp
    User t-zukawa
    GatewayPorts no
    DynamicForward 1080
    ServerAliveInterval 60

Host *
    User t-zukawa
    ServerAliveInterval 300
    IdentityFile ~/.ssh/soflab
    # Macの場合は以下の記述をしておくことでssh keyのパスフレーズ入力を初回以外スキップできる
    UseKeychain yes
    AddKeysToAgent yes
