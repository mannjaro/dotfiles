Host salmon
    HostName salmon.se.is.kit.ac.jp
    ProxyJump puffer

Host saury
    HostName saury.se.is.kit.ac.jp
    ProxyJump puffer

Host lobster
    HostName lobster.se.is.kit.ac.jp
    ProxyJump puffer

Host tuna
    HostName tuna.se.is.kit.ac.jp
    ProxyJump puffer

Host sel # ← just an identifier
    HostName se.is.kit.ac.jp
    User t-zukawa
    GatewayPorts no
    DynamicForward 1080
    ServerAliveInterval 60

Host *
    User t-zukawa
    ServerAliveInterval 300
    IdentityFile ~/.ssh/sl_key
    # Macの場合は以下の記述をしておくことでssh keyのパスフレーズ入力を初回以外スキップできる
    UseKeychain yes
    AddKeysToAgent yes
