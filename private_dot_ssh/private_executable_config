Host fab_web
    HostName www.monodukuri.kit.ac.jp
    User mannjaro
    Port 22
    IdentityFile ~/.ssh/manjaro_rsa

Host rpi
    HostName 192.168.11.27
    User pi
    Port 22
    IdentityFile ~/.ssh/rpi_key

Host puffer
    HostName se.is.kit.ac.jp
    ControlMaster auto
    ControlPersist 10m

Host ayu
    HostName ayu.se.is.kit.ac.jp
    ProxyJump puffer

